group YassTree;

assign(name, value) ::= <<--<name>: <value>;>>

number(text) ::= "<text>"
string(text) ::= "<text>"

stylesheet(imports, assignments, forLoops) ::= <<
<imports; separator="\n">
:root {
  <assignments; separator="\n">
}
<forLoops; separator="\n">
>>

printImport(value) ::= "@import(<value>)"

list(values) ::= <<
<values; separator="\n">
>>

forLoop(elements, content) ::= "<elements:{element | <element> <content>;}>"