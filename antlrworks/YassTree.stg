group YassTree;

assign(name, value) ::= <<--<name>: <value>;>>

number(text) ::= "<text>"
string(text) ::= "<text>"

stylesheet(statements) ::= <<
<statements; separator="\n">
>>

printImport(value) ::= "@import(<value>)"

list(values) ::= <<
<values; separator="\n">
>>

forLoop(elements, content) ::= "<elements:{element | <element> <content>;}>"

printSelector(names) ::= <<
<names; separator="\n">
>>