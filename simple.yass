color = "blue";
font_size = "1px";
pseudo = "hover";
dict = {
  "primary": "blue",
  "secondary": "red"
};

btn = (color) {
  color: ${color};
  font-size: 1px;
};

$foreach(dict) {
.class-${key} {
  $btn(value)
}
}

.main[target=${color}]:${pseudo}, .class[target][target] {
  color: ${color};
}




