background_color = "red";
text_color = "white";
font_size = "1px";
list = ["primary", "secondary"];

.main p, div {
  background-color: ${background_color};
  & p {
    font-size: ${font_size};
  }
}

.main-${font_size} {
  background-color: black;
}

$foreach(list){
  .prova {
    background-color: ${text_color};
  }
}