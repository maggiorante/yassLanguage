colors = {
  "primary": #E6EED6,
  "secondary": #DDE2C6,
  "danger": #BBC5AA,
  "black": #A72608,
  "white": #090C02
};

btn = (color) {
  color: ${color};
  border: 1px solid black;
  border-radius: 1px;
  outline: none;
};

& body, & p, div {
  $foreach(colors) {
    .btn-${index} {
      $btn(value);
      &:hover {
        color: white;
      }
    }
  }
}