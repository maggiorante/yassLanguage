list = ["primary", "secondary"];
dict = {
  "color": "black",
  "background-color": "white"
};

$foreach(list) {
  .prova-${value} {
    background-color: black;
  }
}

list = ["primario", "secondario"];

$foreach(list) {
  .prova-${value} {
    background-color: black;
  }
}

$genera-btn(nome, dict, rounded) {
  .${nome} {
    $foreach(dict) {
      $if(rounded and key == "border-radius"){
        border-radius: 1px;
      } else {
        ${key}: ${value};
      }
    }
  }
}

