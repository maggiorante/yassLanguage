colors = {
  "primary": #E6EED6,
  "secondary": #DDE2C6,
  "danger": #BBC5AA,
  "black": #A72608,
  "white": #090C02
};

sizes = {
  "big": 200px,
  "medium": 100px,
  "small": 50px
};

body {
  color: black;
  $foreach(colors) {
    .btn-${index} {
      color: ${value};
      $foreach(i, size : sizes) {
        ${i}: ${size};
      }

      $foreach(i, size : sizes) {
        ${i}: ${size};
      }
    }
  }
}